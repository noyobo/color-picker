{
  "name": "rc-color-picker",
  "version": "1.0.0",
  "description": "color-picker ui component for react",
  "keywords": [
    "react",
    "react-component",
    "react-color-picker",
    "color-picker"
  ],
  "homepage": "http://github.com/react-component/color-picker",
  "author": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:react-component/color-picker.git"
  },
  "bugs": {
    "url": "http://github.com/react-component/color-picker/issues"
  },
  "licenses": "MIT",
  "main": "./lib/index",
  "files": [
    "index.js",
    "lib/"
  ],
  "config": {
    "port": 8000
  },
  "scripts": {
    "build": "rc-tools run build",
    "precommit": "rc-tools run precommit",
    "less": "rc-tools run less",
    "gh-pages": "rc-tools run gh-pages",
